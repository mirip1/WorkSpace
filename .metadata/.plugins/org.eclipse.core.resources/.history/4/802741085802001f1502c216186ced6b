package practica1;

public class CostePersonal {

  static float costeDelPersonal(Trabajador trabajadores[]) {
    float costeFinal = 0;
    Trabajador trabajador;
    for(int i = 0; i<trabajadores.lenght;i++) {
      tranajador = trabajadores[i];
        
      if (trabajador.trab() ==TippoTrabajador.DIRECTOR || trabajador.trab() == TipoTrabajadorr.SUBDIRECTOR) {
        costeFinal += trabajador.getNom();
        
        
      } 
    }
    
  }
  
}
